<apex:page Controller="extattachfile">
      <apex:form >
          <apex:pageBlock title="Submit File">
             <apex:pageMessages />
              <apex:pageBlockButtons >
                  <apex:commandButton value="Save" action="{!saveApplicant}"/>
              </apex:pageBlockButtons>
              <apex:pageBlockSection >
                  <apex:inputField value="{!objApplicant.origin}"/>
                  <apex:inputfile value="{!objAttachment.body}" filename="Test"></apex:inputfile>
              </apex:pageBlockSection>
          </apex:pageBlock>
      </apex:form>
  </apex:page>