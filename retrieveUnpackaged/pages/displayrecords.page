<!--Display the records using stadard controller-->

<apex:page showHeader="false" standardcontroller="account" recordSetVar="recs"  >

<apex:pageblock title=" Block One  " >
    <apex:pageblocksection title=" Lead Records Section ">
    
            <apex:pageblocktable value="{!recs}"  var="item"   >
                <apex:column value="{!item.name}"/>
                <apex:column value="{!item.phone}"/>
                <apex:column value="{!item.Industry}"/>
            
            </apex:pageblocktable>
    
    </apex:pageblocksection>

</apex:pageblock>

</apex:page>