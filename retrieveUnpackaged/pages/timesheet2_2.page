<apex:page controller="timesheet2" renderAs="pdf" applyBodyTag="false">
  <head>
        <style> 
        @page {
size:landscape;
}
 
            body { font-family: 'Arial Unicode MS';font: 14px; }
            .borderst{ border-top:1px solid grey; }  
        </style>
    </head>
 <body>
 <table border="0" cellpadding="5" cellspacing="0" width="100%">
  <tr><td colspan="10" ></td></tr>
 <tr><td colspan="10" align="center"><span style="font-size:22px">Monthly Time Sheet </span></td></tr>
  <tr><td colspan="10"><b>Ascott Sales Team</b> </td></tr>
    <tr><td colspan="5"><b>Name :   Edwin Sta Maria </b></td><td colspan="5"><b>Start date:{!startdate} </b></td></tr>
     <tr><td colspan="5"><b>Territory Number:   A 402</b> </td><td colspan="5"><b>End date: {!enddate}</b></td></tr>
      <tr><td colspan="10"><b>Territory Name:     North Brisbane</b> </td></tr>
       <tr style="background:rgb(216, 216, 216);font-weight:bold"><td >Date </td><td >Weekend </td><td >Field Day</td><td >State sales Meeting </td><td >Client Training/ Conference </td><td >Ascott Meeting </td><td >Public Holiday</td><td >Annual Leave</td><td >Sick Leave </td><td >Total </td></tr>
       <apex:repeat value="{!mapper}" var="key"><tr><td>{!key}</td><apex:repeat value="{!mapper[key]}" var="lstval"><td>{!lstval}</td></apex:repeat></tr></apex:repeat>
       <apex:repeat value="{!lastmap}" var="key"><tr style="background:rgb(216, 216, 216);font-weight:bold"><td>{!key}</td><apex:repeat value="{!lastmap[key]}" var="lstval"><td>{!lstval}</td></apex:repeat></tr></apex:repeat>
<tr><td colspan="10" align="Right">Total Days in Month:{!i}</td></tr>
 <tr><td colspan="10" ></td></tr>
 <tr><td colspan="10" ></td></tr>
<tr><td colspan="10" ></td></tr>  
<tr style="border-top:1px solid grey;"><td colspan="5" ></td><td colspan="5" align="left" class="borderst">Employee Signature</td></tr> 
<tr><td colspan="10" ></td></tr> 
<tr><td colspan="10" ></td></tr>
<tr><td colspan="10" ></td></tr>  
<tr style="border-top:1px solid grey;"><td colspan="5" ></td><td colspan="5" align="left" style="border-top:1px solid grey;">Manager Signature</td></tr> 
</table>
 </body>
</apex:page>